# Configuration for dyn-advisor
# Copy this file to .env and configure as needed

# Execution control (default: false for safety)
# Set to true to allow execution of Dynamo graphs via CLI
ALLOW_EXECUTION=false

# Path to Dynamo CLI executable (if execution is enabled)
# Example: /path/to/DynamoSandbox.exe or /usr/local/bin/dynamo
DYNAMO_CLI_PATH=

# Path to the repository of Dynamo graphs to index
# Default: ./graphs
GRAPH_REPO_PATH=./graphs

# Path to documentation files for context
# Default: ./docs
DOCS_PATH=./docs

# Logging level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO

# Log file path
LOG_FILE=./dyn-advisor.log
